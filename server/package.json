{
    "name": "baucua-server",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon src/index.js --exec babel-node",
        "build": "babel src -d dist",
        "prestart": "babel src -d dist",
        "start": "node dist/index.js NODE_ENV=development",
        "start-youtube": "node dist/youtube-index.js NODE_ENV=production",
        "prod": "pm2 start index.js",
        "stop-prod": "pm2 remove index",
        "test": "jest --verbose",
        "db:create": "npx sequelize db:create",
        "db:migrate": "npx sequelize db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "delay": "^3.0.0",
        "express": "^4.16.2",
        "jest": "^22.1.4",
        "jest-each": "^0.3.1",
        "morgan": "^1.9.0",
        "multer": "^1.4.2",
        "mysql2": "^2.2.5",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "pm2": "^2.9.1",
        "request": "^2.83.0",
        "request-promise": "^4.2.2",
        "sequelize": "^6.3.5",
        "sequelize-cli": "^6.2.0",
        "socket.io": "^2.0.4"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-flow": "^6.23.0",
        "flow-bin": "^0.66.0",
        "nodemon": "^1.17.5"
    }
}
